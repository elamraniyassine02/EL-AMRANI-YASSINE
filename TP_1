package TP;
import java.util.Scanner;
public class TP1ex {

	public static void main(String[] args) {
		scanner scanner = new Scanner(System.in);

        System.out.println("Veuillez entrer une valeur pour a:");
        int a = scanner.nextInt();

        int motsecret = 25 ;

        // Compare
        if (a < motsecret) {
            System.out.println("le numero est inferieur au numero secret");
        } else if (a > motsecret) {
            System.out.println("Le numero est superieur au numero secret ");
        } else {
            System.out.println("Bravo vous avez gagne");
        }

        scanner.close();
		
	}

}
